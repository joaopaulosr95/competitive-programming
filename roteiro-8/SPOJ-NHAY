#include <iostream> // std::cout
#include <algorithm> // std::sort
#include <vector> // std::vector
#include <string>
#include <iterator>
#include <string.h>
#include <cstdio>

using namespace std;

void verificaPadrao(std::string padrao,std::string texto,int j){
		int tamP = padrao.length();
		std::string auxPadrao = padrao;
		int cnt=0;
	//	cout << " ENTREI NA FUNCAO " << endl;
		for(int z=j;z<j+tamP;z++){
		//	cout << "ENTREI NO FOR " << endl;
			if(texto.at(z)==auxPadrao.at(0)) {
				cnt=cnt+1;
			//	cout << "++ no cnt" << cnt << endl;
				auxPadrao.erase(0,1);
			//	cout << "FIZ ERASE " << endl;
			}

		}
	//	cout << cnt << " " << tamP << endl;
		if(cnt==tamP) cout << j << endl;


}


int main(){
		int tamP;
while((scanf("%d", &tamP))!=EOF){
    std::string padrao;
    cin >> padrao;
    std::string texto;
    cin >> texto;
		int tamT = texto.length();
		int diff = tamT-tamP+1;
		int aux=0;
		for(int i=0;i<diff;i++) {
				if(texto.at(i)==padrao.at(0)){
					aux++;
					verificaPadrao(padrao,texto,i);
				}
		}
			if(aux==0) cout << endl;
}


	return 0;
}
