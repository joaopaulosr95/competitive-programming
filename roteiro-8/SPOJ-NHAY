#include <iostream>     // std::cout
#include <algorithm>    // std::sort
#include <vector>       // std::vector
#include <string>
#include <iterator>
#include <string.h>

using namespace std;

int main(){

	int tam;
	std::string padrao,texto;
	
	while(cin >> tam){
	cin >> padrao;
	cin >> texto;
	int tamP = padrao.size();
	int tamT = texto.size();
	char arrayPadrao[tamP];
	char arrayTexto[tamT];

	strcpy(arrayPadrao, padrao.c_str());
	strcpy(arrayTexto, texto.c_str());

	int cnt=0;

		for(int j=0;j<tamT;j++){
			if(arrayTexto[j]==arrayPadrao[0]){
				int aux0=0;
				int k=j;
					while(arrayTexto[k]==arrayPadrao[aux0] && aux0<tamP){				
						k++;
						aux0++;
					}

				if(aux0==tamP) {
					cnt++;
					cout << j << endl;
				}
			} 
		}
		if(cnt==0) cout << endl;
	}
	
	return 0;
}
